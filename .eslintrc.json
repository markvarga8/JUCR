{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier", "unused-imports"],
  "rules": {
    "prettier/prettier": ["error", {}, { "usePrettierrc": true }],
    "unused-imports/no-unused-imports": "error"
  }
}
